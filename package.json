{
  "name": "victory-pie",
  "version": "0.1.1",
  "description": "D3 pie & donut chart component for React",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/FormidableLabs/victory-pie.git"
  },
  "author": "Angela Nicholas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FormidableLabs/victory-pie/issues"
  },
  "homepage": "https://github.com/FormidableLabs/victory-pie",
  "scripts": {
      "postinstall": "builder run npm:postinstall",
      "preversion": "builder run npm:preversion",
      "version": "builder run npm:version",
      "test": "builder run npm:test",
      "dev-docs": "npm run generate-docs & webpack-dev-server --port 3000 --config docs/webpack.config.dev.js --content-base docs",
      "generate-docs": "node_modules/react-docgen/bin/react-docgen.js src/components/victory-pie.jsx -o docs/victory-pie.json",
      "hot-docs": "npm run generate-docs & webpack-dev-server --port 3000 --config docs/webpack.config.hot.js --hot --content-base docs",
      "build-static-docs": "webpack --config docs/webpack.config.static.js --progress",
      "push-gh-pages": "git subtree push --prefix docs/build origin gh-pages"
    },
    "dependencies": {
      "builder": "~2.1.3",
      "builder-react-component": "~0.1.0",
      "d3": "^3.5.6",
      "lodash": "^3.9.3",
      "radium": "^0.14.1",
      "victory-animation": "^0.0.10",
      "victory-util": "^2.0.1"
    },
    "devDependencies": {
      "babel": "^5.5.8",
      "babel-core": "^5.5.8",
      "babel-loader": "^5.3.2",
      "builder-react-component-dev": "~0.1.0",
      "chai": "^3.2.0",
      "clean-webpack-plugin": "^0.1.4",
      "ecology": "~1.0.4",
      "file-loader": "^0.8.4",
      "json-loader": "^0.5.3",
      "mocha": "^2.2.5",
      "raw-loader": "^0.5.1",
      "react": "0.14.x",
      "react-docgen": "^2.4.0",
      "react-dom": "0.14.x",
      "react-hot-loader": "^1.2.8",
      "sinon": "^1.15.4",
      "sinon-chai": "^2.8.0",
      "static-site-generator-webpack-plugin": "^1.2.0",
      "webpack": "^1.10.0",
      "webpack-dev-server": "^1.10.0",
      "webpack-stats-plugin": "^0.1.1"
    }
  }
