{
  "name": "victory-util",
  "version": "1.1.0",
  "description": "Victory helpers and utility functions",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/FormidableLabs/victory-util.git"
  },
  "author": "Brian Beck",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FormidableLabs/victory-util/issues"
  },
  "homepage": "https://github.com/FormidableLabs/victory-util",
  "scripts": {
    "postinstall": "node -e \"require('fs').stat('lib', function(e,s){process.exit(e || !s.isDirectory() ? 1 : 0)})\" || npm run build-lib",
    "preversion": "npm run check",
    "version": "npm run clean && npm run build && git add -A dist",
    "clean-dist": "rimraf dist",
    "build-dist-min": "webpack --config webpack.config.js",
    "build-dist-dev": "webpack --config webpack.config.dev.js",
    "build-dist": "npm run clean-dist && npm run build-dist-min && npm run build-dist-dev",
    "clean-lib": "rimraf lib",
    "build-lib": "npm run clean-lib && babel src -d lib",
    "clean": "npm run clean-lib && npm run clean-dist",
    "build": "npm run build-lib && npm run build-dist",
    "server-test": "webpack-dev-server --port 3001 --config webpack.config.test.js --colors",
    "dev": "concurrent -kr 'npm run server-dev' 'npm run server-test'",
    "hot": "concurrent -kr 'npm run server-hot' 'npm run server-test'",
    "lint-server": "eslint -c .eslintrc-server *.js",
    "lint-client": "eslint --ext .js,.jsx -c .eslintrc-client src",
    "lint-client-test": "eslint --ext .js,.jsx -c .eslintrc-client-test src test/client",
    "lint": "npm run lint-server && npm run lint-client && npm run lint-client-test",
    "test-frontend": "node node_modules/karma/bin/karma start karma.conf.js",
    "test-frontend-ci": "node node_modules/karma/bin/karma start --browsers PhantomJS,Firefox karma.conf.coverage.js",
    "test-frontend-cov": "node node_modules/karma/bin/karma start karma.conf.coverage.js",
    "test-frontend-dev": "node node_modules/karma/bin/karma start karma.conf.dev.js",
    "test": "npm run test-frontend && echo 'TODO Server 13, Integration 12 tests.'",
    "test-ci": "npm run test-frontend-ci",
    "test-cov": "npm run test-frontend-cov",
    "test-dev": "npm run test-frontend-dev",
    "check": "npm run lint && npm run test",
    "check-ci": "npm run lint && npm run test-ci",
    "check-cov": "npm run lint && npm run test-cov",
    "check-dev": "npm run lint && npm run test-dev"
  },
  "dependencies": {
    "babel": "^5.8.21",
    "babel-core": "^5.8.22",
    "babel-loader": "^5.3.2",
    "css-loader": "^0.20.1",
    "file-loader": "^0.8.4",
    "lodash": "^3.10.1",
    "reduce-css-calc": "^1.2.0",
    "rimraf": "^2.4.0",
    "style-loader": "^0.12.4",
    "url-loader": "~0.5.5",
    "webpack": "^1.11.0"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.3",
    "chai": "^3.2.0",
    "concurrently": "^0.1.1",
    "eslint": "^1.7.1",
    "eslint-config-defaults": "^7.0.1",
    "eslint-plugin-filenames": "^0.1.2",
    "eslint-plugin-react": "^3.6.2",
    "isparta-loader": "^1.0.0",
    "karma": "^0.13.11",
    "karma-chrome-launcher": "^0.2.0",
    "karma-coverage": "^0.5.2",
    "karma-firefox-launcher": "^0.1.6",
    "karma-ie-launcher": "^0.2.0",
    "karma-mocha": "^0.2.0",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-phantomjs-shim": "^1.1.1",
    "karma-safari-launcher": "^0.1.1",
    "karma-sauce-launcher": "^0.2.14",
    "karma-spec-reporter": "0.0.20",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.2.5",
    "opener": "^1.4.1",
    "phantomjs": "^1.9.18",
    "react": "^0.14.0",
    "react-addons-test-utils": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-hot-loader": "^1.2.8",
    "sinon": "^1.16.1",
    "sinon-chai": "^2.8.0",
    "webpack-dev-server": "^1.10.0"
  }
}
