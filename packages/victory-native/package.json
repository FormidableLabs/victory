    "victory-brush-line": "^36.5.3",
    "victory-candlestick": "^36.5.3",
    "victory-chart": "^36.5.3",
    "victory-core": "^36.5.3",
    "victory-create-container": "^36.5.3",
    "victory-cursor-container": "^36.5.3",
