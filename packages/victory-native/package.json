    "victory-zoom-container": "^36.5.3"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "react-native": "^0.65.1",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-svg": "^12.1.1"
  },
  "scripts": {
    "build": "wireit",
    "format": "wireit",
    "lint": "wireit",
    "jest": "wireit"
