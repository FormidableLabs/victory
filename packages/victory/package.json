{
  "name": "victory",
  "version": "36.5.3",
  "description": "Data viz for React",
  "keywords": [
    "data visualization",
    "React",
    "d3",
    "charting"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/formidablelabs/victory.git"
  },
  "homepage": "https://formidable.com/open-source/victory",
  "source": "src/index.js",
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "author": "Formidable",
  "license": "MIT",
  "dependencies": {
    "victory-area": "^36.5.3",
    "victory-axis": "^36.5.3",
    "victory-bar": "^36.5.3",
    "victory-box-plot": "^36.5.3",
    "victory-brush-container": "^36.5.3",
    "victory-brush-line": "^36.5.3",
    "victory-candlestick": "^36.5.3",
    "victory-canvas": "^36.5.3",
    "victory-chart": "^36.5.3",
    "victory-core": "^36.5.3",
    "victory-create-container": "^36.5.3",
    "victory-cursor-container": "^36.5.3",
    "victory-errorbar": "^36.5.3",
    "victory-group": "^36.5.3",
    "victory-histogram": "^36.5.3",
    "victory-legend": "^36.5.3",
    "victory-line": "^36.5.3",
    "victory-pie": "^36.5.3",
    "victory-polar-axis": "^36.5.3",
    "victory-scatter": "^36.5.3",
    "victory-selection-container": "^36.5.3",
    "victory-shared-events": "^36.5.3",
    "victory-stack": "^36.5.3",
    "victory-tooltip": "^36.5.3",
    "victory-voronoi": "^36.5.3",
    "victory-voronoi-container": "^36.5.3",
    "victory-zoom-container": "^36.5.3"
  },
  "peerDependencies": {
    "react": ">=16.6.0"
  },
  "scripts": {
    "build": "wireit",
    "build:lib": "wireit",
    "build:lib:esm": "wireit",
    "build:lib:cjs": "wireit",
    "build:dist": "wireit",
    "build:dist:dev": "wireit",
    "build:dist:min": "wireit"
  },
  "wireit": {
    "build": {
      "dependencies": [
        "build:lib",
        "build:dist"
      ]
    },
    "build:lib": {
      "dependencies": [
        "build:lib:esm",
        "build:lib:cjs"
      ]
    },
    "build:lib:esm": {
      "command": "nps build:lib:esm",
      "files": [
        "src/**",
        "../../.babelrc.build.js"
      ],
      "output": [
        "es/**"
      ]
    },
    "build:lib:cjs": {
      "command": "nps build:lib:cjs",
      "files": [
        "src/**",
        "../../.babelrc.build.js"
      ],
      "output": [
        "lib/**"
      ]
    },
    "build:dist": {
      "dependencies": [
        "build:dist:dev",
        "build:dist:min"
      ]
    },
    "build:dist:dev": {
      "command": "nps build:dist:dev",
      "files": [
        "src/**",
        "../../.babelrc.build.js",
        "../../config/webpack.config.js",
        "../../config/webpack.config.dev.js"
      ],
      "output": [
        "victory*.js",
        "!victory*.min.js*"
      ]
    },
    "build:dist:min": {
      "command": "nps build:dist:min",
      "files": [
        "src/**",
        "../../.babelrc.build.js",
        "../../config/webpack.config.js"
      ],
      "output": [
        "victory*.min.js*"
      ]
    }
  },
  "sideEffects": false
}